          <xsd:documentation>Flight altitude and type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="speed" type="nxce:speedType">
        <xsd:annotation>
          <xsd:documentation>Flight speed and type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ncsmRouteData" type="ncsmFlightRouteDataType">
        <xsd:annotation>
          <xsd:documentation>The NCSM Route Data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
