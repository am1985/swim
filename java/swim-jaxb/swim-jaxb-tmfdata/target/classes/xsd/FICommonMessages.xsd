      <xsd:element name="elementType" type="fce:elementTypeType">
        <xsd:annotation>
          <xsd:documentation>Type of element covered by program: APT or FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="altitudesIncluded" type="fce:altitudeRangeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Included altitude range.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="adlTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Time of last ADL report.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="compressionPeriod" type="fce:timePeriodType">
        <xsd:annotation>
          <xsd:documentation>Start and end time of compression.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cumulativeProgramPeriod" type="fce:timePeriodType">
        <xsd:annotation>
          <xsd:documentation>The absolute start and end time of all TMIs in the program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="exemptCTOPList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Cumulative Program Period describes the total duration of an AFP after the end date has been revised. In other words it starts with the initial AFP start time and ends with the revised AFP end time.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctop" type="ctopNameType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>The CTOP name as defined by the traffic manager.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="slotHoldOverrideList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of carriers to override slot holds.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="carrier" type="xsd:string" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Carriers as entered by the user.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="impactingCondition" type="fce:reasonType">
        <xsd:annotation>
          <xsd:documentation>Impacting conditions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryValidPeriod" type="fce:timePeriodType">
        <xsd:annotation>
          <xsd:documentation>Start and end times of the valid period.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="currentDelays" type="fce:delayInfoType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Current delays.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="anticipatedDelays" type="fce:delayInfoType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Anticipated delays.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fca">
    <xsd:annotation>
      <xsd:documentation>Defines FCA message content.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="fcaId" type="fcaIdType">
        <xsd:annotation>
          <xsd:documentation>System generated unique FCA identifier</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaName" type="fce:fcaNameType">
        <xsd:annotation>
          <xsd:documentation>The FCA name as defined by the traffic manager.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaDomain" type="nxce:tmiVisibilityDomainType">
        <xsd:annotation>
          <xsd:documentation>The domain of the FXA; PUBLIC, PRIVATE, SHARED, LOCAL.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmiStatus" type="fce:tmiStatusType">
        <xsd:annotation>
          <xsd:documentation>The status of the FCA TMI; NEW, UPDATED, or DELETED.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lastUpdate" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The time the FCA was last updated.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lastUpdateWorkstationId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Last workstation that updated FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lastUpdateSiteId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Last site that updated the FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="createWorkstationId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Workstation that created the FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="createSiteId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Site that created the FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="sharedSiteIds" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Sharing Sites.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaReason" type="fce:reasonType">
        <xsd:annotation>
          <xsd:documentation>Reason for FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="feaFcaType">
        <xsd:annotation>
          <xsd:documentation>Type of FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FEA"/>
            <xsd:enumeration value="FCA"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="colorId" type="fce:colorIdType">
        <xsd:annotation>
          <xsd:documentation>Color code for display purposes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="startTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>FCA start time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="endTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>FCA end time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="extendedFeaFlag" type="booleanFlag">
        <xsd:annotation>
          <xsd:documentation>Extended FEA indicator.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lookAheadTime">
        <xsd:annotation>
          <xsd:documentation>Time range in hours from current time over which TFMS will determine the FCA flights.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="24"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="fsmEligibleFlag" type="booleanFlag">
        <xsd:annotation>
          <xsd:documentation>FSM Eligible: True or False.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="polygon" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Data elements for a custom polygon FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ceiling" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Upper altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="floor" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Lower altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="points" type="pointsType">
              <xsd:annotation>
                <xsd:documentation>The points of the polygon that define the shape of the FCA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="direction" type="directionType">
              <xsd:annotation>
                <xsd:documentation>FCA direction in degrees 0 through 360.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="speed" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>FCA speed in knots.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="drawing" type="booleanFlag">
              <xsd:annotation>
                <xsd:documentation>For moving FCAs, defines how to associate the FCA position as represented by points to a time. If true, the points represent the position at start time. If false, points represents the position at the time it was last updated (LASTUPDATETIME).</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="line" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Data elements for a multi-segment line FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ceiling" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Upper altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="floor" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Lower altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="points" type="pointsType">
              <xsd:annotation>
                <xsd:documentation>The points of the line segments that define the shape of the FCA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="direction" type="directionType">
              <xsd:annotation>
                <xsd:documentation>FCA direction in degrees 0 through 360.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="speed" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>FCA speed in knots.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="drawing" type="booleanFlag">
              <xsd:annotation>
                <xsd:documentation>For moving FCAs, defines how to associate the FCA position as represented by points to a time. If true, the points represent the position at start time. If false, points represents the position at the time it was last updated (LASTUPDATETIME).</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="circle" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Data elements for a circle FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ceiling" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Upper altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="floor" type="fce:acAltType">
              <xsd:annotation>
                <xsd:documentation>Lower altitude of FCA in 100s of feet.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="points" type="pointsType">
              <xsd:annotation>
                <xsd:documentation>The points of the circle that define the shape of the FCA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="center" type="fce:coordinateType">
              <xsd:annotation>
                <xsd:documentation>The center of the circle.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="radius" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>The radius of the circle.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="nas" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Data elements for a NAS element FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:choice>
              <xsd:element name="airportName" type="nxce:airportIdType">
                <xsd:annotation>
                  <xsd:documentation>Airport Identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="sectorName" type="nxce:sectorIdentifierType">
                <xsd:annotation>
                  <xsd:documentation>The sector identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="baseSector" type="nxce:sectorIdentifierType">
                <xsd:annotation>
                  <xsd:documentation>The base sector identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="traconName">
                <xsd:annotation>
                  <xsd:documentation>Indicates that NAS element type is TRACON and provides the TRACON name.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:pattern value="\w{3,6}"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="baseTracon">
                <xsd:annotation>
                  <xsd:documentation>Indicates the element category is a baseline-state TRACON and provides the TRACON name.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:pattern value="\w{3,6}"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="artccName" type="fce:centerIdType">
                <xsd:annotation>
                  <xsd:documentation>the ARTCC Center Identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="fixName" type="nxce:fixNameType">
                <xsd:annotation>
                  <xsd:documentation>The fix identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="suaName" type="xsd:string">
                <xsd:annotation>
                  <xsd:documentation>The SUA identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:choice>
            <xsd:element name="ceiling" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Upper altitude of FEA/FCA (100s feet).</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="600"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="floor" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Lower altitude of FEA/FCA (100s feet).</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="600"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="fcaDisplayPreferences" type="fcaDisplayPreferencesType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The FCA display preferences.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="primaryFilter" type="primaryFilterType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The primary filter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="secondaryFilter" type="secondaryFilterType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The secondary filter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="fcaIdType">
    <xsd:annotation>
      <xsd:documentation>System generated unique identifier of FEA/FCA. Format is fca.{sitename}.{hostname}.{yyyyMMddhhmmss}</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="fca\.[A-Za-z0-9]{1,8}\.\w{1,8}\.\d{14}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="booleanFlag">
    <xsd:annotation>
      <xsd:documentation>TRUE or FALSE.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TRUE"/>
      <xsd:enumeration value="FALSE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="pointsType">
    <xsd:annotation>
      <xsd:documentation>The points of a polygon that define the shape of an FEA/FCA. Up to sixty points.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="point" type="fce:coordinateType" maxOccurs="60">
        <xsd:annotation>
          <xsd:documentation>Each point contains a latitude and longitude.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="includedAircraftType">
    <xsd:annotation>
      <xsd:documentation>The propulsion type of the included aircrafts.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="JET DEP ONLY"/>
      <xsd:enumeration value="PROP DEP ONLY"/>
      <xsd:enumeration value="JET AND PROP ONLY"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="directionType">
    <xsd:annotation>
      <xsd:documentation>Direction of movement in degrees for a moving FEA/FCA.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="360"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="fcaDisplayPreferencesType">
    <xsd:annotation>
      <xsd:documentation>Preferences definitions.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="fcaTimeline">
        <xsd:annotation>
          <xsd:documentation>FCA timeline preferences.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="visibleTimeRange" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>Visible time range.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="flightCountUnit">
              <xsd:annotation>
                <xsd:documentation>Flight count description.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="PEAK"/>
                  <xsd:enumeration value="TOTAL"/>
                  <xsd:enumeration value="ENTRY"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="selectedTimeRange" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation>Selected time range.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="listSort" minOccurs="3" maxOccurs="3">
        <xsd:annotation>
          <xsd:documentation>Sorting options for a sort level. There are always containers for each of the three levels, even if some or all or empty. Level 1 is the highest level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="sortLevel">
              <xsd:annotation>
                <xsd:documentation>The sort level.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="1"/>
                  <xsd:maxInclusive value="3"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="columnHeader" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The column header.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="sortOrder">
              <xsd:annotation>
                <xsd:documentation>The sorting order.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="ASCENDING"/>
                  <xsd:enumeration value="DESCENDING"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="columnDisplayContainer">
        <xsd:annotation>
          <xsd:documentation>All of the columns to be displayed by default in the FEA/FCA dynamic list.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="columnDisplay" type="xsd:string" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>The column display.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="traverseListType">
    <xsd:annotation>
      <xsd:documentation>List of center, sector and/or fix names.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="center" type="fce:centerIdType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The Center Identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="sector" type="nxce:sectorIdentifierType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The Sector Identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fix" type="nxce:fixNameType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The Fix Identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="useAirwayListType">
    <xsd:annotation>
      <xsd:documentation>List of airway names.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="airway" type="nxce:airwayNameType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Name of airway.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="locatedListType">
    <xsd:annotation>
      <xsd:documentation>List of center and/or sector names.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="center" type="fce:centerIdType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The center identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="sector" type="nxce:sectorIdentifierType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The sector identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="aircraftTypeListType">
    <xsd:annotation>
      <xsd:documentation>List of aircraft types.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraft" type="nxce:typeOfAircraftType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The aircraft type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="remarksListType">
    <xsd:annotation>
      <xsd:documentation>List of remarks.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="remarks" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Textual remarks.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="headingType">
    <xsd:annotation>
      <xsd:documentation>A heading followed by a tolerance. The heading is degrees from 0 to 360. The tolerance is degrees from 0 to 180.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="heading">
        <xsd:annotation>
          <xsd:documentation>The heading angle.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="360"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="tolerance">
        <xsd:annotation>
          <xsd:documentation>The tolerance angle.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="180"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="aircraftIdListType">
    <xsd:annotation>
      <xsd:documentation>List of aircraft IDs (call signs).</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraftId" type="xsd:string" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>The aircraft identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="anyAllConditionsType">
    <xsd:annotation>
      <xsd:documentation>None, Any and All conditions list.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="departsAny" type="fce:centerAirportListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of departure airports/centers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="departsNone" type="fce:centerAirportListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of departure airports/centers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrivesAny" type="fce:centerAirportListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of arrival airports/centers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrivesNone" type="fce:centerAirportListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of arrival airports/centers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="traverseAny" type="traverseListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of traverse.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="traverseAll" type="traverseListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -All- of traverse.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="traverseNone" type="traverseListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of traverse.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="useAirwayAny" type="useAirwayListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of use airways.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="useAirwayAll" type="useAirwayListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -All- of use airways.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="useAirwayNone" type="useAirwayListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of use airways.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="locatedAny" type="locatedListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of locate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="locatedNone" type="locatedListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of locate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="typeAny" type="aircraftTypeListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of aircraft type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="typeNone" type="aircraftTypeListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of aircraft type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarksAny" type="remarksListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of remarks.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarksAll" type="remarksListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -All- of remarks.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarksNone" type="remarksListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -None- of remarks.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="headingIs" type="headingType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Heading is.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="headingNot" type="headingType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Heading is not.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftIdAny" type="aircraftIdListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List -Any- of aircraft identifiers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftIdNone" type="aircraftIdListType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List None- of aircraft identifiers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightLevel" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flight levels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="upper">
              <xsd:annotation>
                <xsd:documentation>Upper flight level in hundreds of feet.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="lower">
              <xsd:annotation>
                <xsd:documentation>Lower flight level in hundreds of feet.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="aircraftCategoryAny" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of aircraft category values.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="category" type="fce:aircraftCategoryType" maxOccurs="3">
              <xsd:annotation>
                <xsd:documentation>Aircraft category: JET, TURBO, or PROP.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="weightClassAny" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of weight class values.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="weightClass" type="fce:weightClassType" maxOccurs="3">
              <xsd:annotation>
                <xsd:documentation>Weight class: LIGHT, HEAVY, or SUPERHEAVY.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="userCategoryAny" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of user category values.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="userCategory" type="fce:userCategoryType" maxOccurs="5">
              <xsd:annotation>
                <xsd:documentation>User category: AIR_TAXI, CARGO, COMMERCIAL, GENERAL_AVIATION, or MILITARY.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="status" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Status of FEA/FCA (ACTIVE or PROPOSED). If active, FEA/FCA includes only airborne flights. If proposed, FEA/FCA includes only flights currently on the ground.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ACTIVE"/>
            <xsd:enumeration value="PROPOSED"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="rvsm" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RVSM compliant status: COMPLIANT or NON-COMPLIANT. If compliant, the FEA/FCA includes only flights that are RVSM compliant at the time they are in the FEA/FCA. If non-compliant, the FEA/FCA includes only flights that are RVSM non-compliant at the time they are in the FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="COMPLIANT"/>
            <xsd:enumeration value="NON-COMPLIANT"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="departureTimeRange" type="fce:timePeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Departure time range.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrivalTimeRange" type="fce:timePeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Arrival time range.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="primaryFilterType">
    <xsd:annotation>
      <xsd:documentation>Primary filter type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="conditions">
        <xsd:annotation>
          <xsd:documentation>Conditions definition.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="anyConditions" type="anyAllConditionsType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Any conditions.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="allConditions" type="anyAllConditionsType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>All conditions.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="secondaryFilterType">
    <xsd:annotation>
      <xsd:documentation>Secondary filter type definition.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="primaryFilterType">
        <xsd:annotation>
          <xsd:documentation>Extends the primary filter type.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element name="filterId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Computer-generated text string that acts as the unique identification of the secondary filter.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="filterName" type="xsd:string" minOccurs="0"/>
          <xsd:element name="fcaId" type="fcaIdType">
            <xsd:annotation>
              <xsd:documentation>System generated unique FCA identifier</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fcaName" type="fce:fcaNameType">
            <xsd:annotation>
              <xsd:documentation>The FCA name as defined by the traffic manager.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="domain" type="nxce:tmiVisibilityDomainType">
            <xsd:annotation>
              <xsd:documentation>FCA Secondary filter visibility domain</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lastUpdate" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>The time the FCA was last updated.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lastUpdateWorkstationId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Last workstation that updated FCA.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="lastUpdateSiteId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Last site that updated the FCA.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="createWorkstationId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Workstation that created the FCA.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="createSiteId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Site that created the FCA.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="priority">
            <xsd:annotation>
              <xsd:documentation>Indicates the priority of a filter (integer, or -1 if no priority is assigned. The value corresponds to the default order in which the filters are displayed and applied.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:integer">
                <xsd:minInclusive value="-1"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="show" type="xsd:boolean" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Flag indicating whether the filter should be shown by
                        default.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="colorId" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Color index indicating the color that ETMS is using to
                        draw the filter. 0 - 35. (NOTE: See Appendix A for a
                        definition of the COLOR_ID values. )</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:int">
                <xsd:maxInclusive value="35"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="airportConfigType">
    <xsd:annotation>
      <xsd:documentation>Airport Configuration/Runway Rate message type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="eventTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Effective event time for this event</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="entryTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Actual entry time of event</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="facility" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Reporting facility or the facility of interest.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="airport" nillable="true">
        <xsd:annotation>
          <xsd:documentation>The airport designator</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="arrRunwayConf" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Arrival runway configuration</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="depRunwayConf" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Departure runway configuration</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="arrRate" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Arrival rate in flights per hour.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="depRate" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Departure rate in flights per hour.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="updateTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Date and time of update</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="weather" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Weather values: VMC, LVMC for low VMD, IMC, and LIMC for low IMC</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="stratAar" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>System generated values prefilled based on airport, airport configuration and weather for N7210.607 airports. Field shall be blank for other airports</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aarAdjust" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation> This field has preset values: Closed Runways/Taxiways, Airport Surface Congestion, Equipment Outages, Fleet Mix, Sunrise/Sunset, Crosswind/Tailwind, Braking Action, LAHSO, Airspace Acceptance/Limitation, Availability of 2 1/2 half miles on Final, Other (free text). aarAdjust can be a max of 36 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="36"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="dynArrEnteredOnPanel" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Indicator if Dynaar was entered on panel. Values: 1=entered on panel; 0=not entered on panel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="adrEnteredOnPanel" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Indicator if Adr was entered on panel. Values: 1=entered on panel; 0=not entered on panel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="airportInFile" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Indicator if Airport in file National Runway Rates table. Values : 0 = airport in file ; 1 = airport not in file</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrUserSpecified" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Arrival runway indicator Values : 1 = User specified other runway ; 0 = Runway in National Runway Rate file</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="depUserSpecified" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Departure runway indicator Values : 1 = User specified other runway ; 0 = Runway in National Runway Rate file</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarksGroupColor" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Remarks grouping color Values: 0 = Blank (no color grouping) 1 = Magenta 2 = Gold 3 = Brown 4 = Black 5 = Cyan 6 = Yellow-Green 7 = Blue</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="enteringFacility" nillable="true">
        <xsd:annotation>
          <xsd:documentation>The entering facility is where the configuration is entered.  Not all facilities have NTML so an associated center or TRACON may enter the information on their behalf.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="remarks" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="deicingType">
    <xsd:annotation>
      <xsd:documentation>Deicing Report message type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="eventTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Effective event time for this event</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="entryTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Actual entry time of event</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="facility" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Reporting facility</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="airport" nillable="true">
        <xsd:annotation>
          <xsd:documentation>The airport designator</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="status" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Indicator if in or out of deicing. Values: 2 = in deicing 3 = out of deicing</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarks" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="restrictionType">
    <xsd:annotation>
      <xsd:documentation>Restriction Report message type</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="eventTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Event time is the time it was determined that the restriction was needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="entryTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Actual entry time of event.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="facility" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Reporting facility</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="5"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="action" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Action Values: 4 = Initiate, 5 = Extend, 6 = Modify, 8 = Cancel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="restrictionId" type="xsd:long">
        <xsd:annotation>
          <xsd:documentation>Unique restriction identifier</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="restrictedNasElements" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Entries consist of a slash ('/') delimited list that can include fixes, NAVAIDS, facilities, airways, and sectors. Or can be user free format text.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="100"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="startTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Start time is the time the restriction started.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="stopTime" type="xsd:dateTime" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Stop Time is when the restriction is over. Zero indicates that no stop time was entered, one indicates stop time was entered.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="airports" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>A slash '/' delimited list of airports referred to be RSTNTYPE 2 (departures) or 3 (arrivals).</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="50"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="aircraftType" nillable="true">
        <xsd:annotation>
          <xsd:documentation>One of ALL, JETS, PROP, or OTHER (which allows free text entry).</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="25"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="restrictionType" type="xsd:integer" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Type of restriction Values: 1 = Enroute 2 = Departures 3 = Arrivals 4= TEXT (Free form text was entered).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitNumber" type="xsd:long" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Miles in trail (MIT) , minutes (MINIT) in trail, altitude (ALT) and speed (SPD)combinations - Note: code handles values in hex. Values: 1 = MIT 2 = MINIT 4 = ALT 8 = SPD 16 = APREQ 32 = DSP 64 = STOP 128 =TBM 256 = SW (severe weather) 512 = NO TIME 1024 = TXT 2048 = not used 4096 = Requestor Tower 8192 = Requestor TRACON 16384= Requestor Center 65536 = Provider Tower 131072 = Provider TRACON 262144 = Provider Center 524288 = Requestor/Provider same Format</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitValue" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Miles in trail value if MITNUM bit 1 is set Minutes in trail value if MITNUM bit 2 is set</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="altitude" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Altitude value if MITNUM 4, 5 and 6 Values: AOA FL999 AOB FL999</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="9"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="speed" type="xsd:integer" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Speed in knots if MITNUM 8, 9 or 10</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="reasonText" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Reason text for restriction</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="80"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="impactedElements" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Entries consist of a slash delimited list that can include fixes, NAVAIDS, facilities, airways, and sectors. Or can be user free format text. These are the elements that are affected by the restriction.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="60"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="qualifier" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Qualifier field in Restriction panel, may be list of values</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="300"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="passback" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Passback indicator - Values: Y or N</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="exclusions" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Allows you to exclude NAS elements. This is a procedural feature which allows exclusion of the NAS element from the restriction.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="25"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="approvalTime" type="xsd:dateTime" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Event time of approval</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="providerStatus" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Current state of provider statuses. The format is:  PROVIDER1/N/#PROVIDER2/N# ...  If there is just a single provider as in most cases, the format would be PROVIDER/N/#.  PROVIDER is the facility ID (e.g.  ZNY) N is an integer indicating:  0 = Provider hasn't taken action; 7 = Provider accepted; 8 = Provider requested conference; 9 = Requester force accepted on behalf of the provider. # is just used as a string delimiter.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1024"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="referenceRestrictionEndTime" type="xsd:string" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>end time prior to extension</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="referenceRestrictionId" type="xsd:long" nillable="true">
        <xsd:annotation>
          <xsd:documentation>Id of modified restriction</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarks" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1024"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="dccRemark" nillable="true" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>DCC remarks</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1024"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="milesInTrailRestrictionType">
    <xsd:annotation>
      <xsd:documentation/>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="includeMit" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>One of true or false. A value of false indicates that the MIT is to be ignored by the RRIABE when determining model results.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitId">
        <xsd:annotation>
          <xsd:documentation>MIT identifier.  Must be unique within the scope of this reroute.  Up to 30 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="30"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="spacingInMiles">
        <xsd:annotation>
          <xsd:documentation>The spacing to be applied to flights in this MIT, in miles. For an FEA/FCA based MIT, this value can be overridden by the spacing applied to flights in a secondary filter.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="EXCLUDE|\d{1,3}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="applyToFlights">
        <xsd:annotation>
          <xsd:documentation>One of ALL, REROUTED, NON-REROUTED. Indicates that the MIT restriction will be applied to all flights that match the restriction criteria or only the rerouted or non-rerouted flights.  ALL and NON-REROUTED are allowed only for FEA/FCA-based MIT.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ALL"/>
            <xsd:enumeration value="REROUTED"/>
            <xsd:enumeration value="NON-REROUTED"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="mitTimeType">
        <xsd:annotation>
          <xsd:documentation>One of MANUAL, AUTO, or FEA_TIMES.  MANUAL means the user has set the start and end time of the MIT manually.  AUTO means that TFMS will compute a start and end time that ensures that all flights in the reroute are subject to the MIT. FEA_TIMES means that TFMS will set the MIT start and end times to the FEA/FCA start and end times.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MANUAL"/>
            <xsd:enumeration value="AUTO"/>
            <xsd:enumeration value="FEA_TIMES"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="mitStartTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Start time for the MIT restriction.  MIT will be applied to any flight that is modeled to cross the MIT restriction point between the start time and the end time.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitEndTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>End time for the MIT restriction.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice minOccurs="0">
        <xsd:element name="mitCrossingSegment">
          <xsd:annotation>
            <xsd:documentation>The points of a multi-segment crossing line drawn on the TSD and used to select the reroute segments affected by the MIT restriction. If present, the crossing segment data is only used by the TSD to draw the crossing line.  It is not used by the backend to determine the segments that are part of the MIT. Each point is a comma-delimited latitude and longitude in hundredths of degrees (i.e., degrees times 100). Latitudes are assumed to be North and longitudes West. Points are separated by spaces. (This is same data structure as the one used for Line FCAs.)
</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:pattern value="(\d{3,5},\d{3,5}[ ]*)+"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="mitNamedFix" type="nxce:fixNameType">
          <xsd:annotation>
            <xsd:documentation>A named fix where the MIT is to be applied.  </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="mitFeaId" type="rriaIdType">
          <xsd:annotation>
            <xsd:documentation>The ID of an FCA/FEA that will be used to specify the flights that will be subject to the MIT restriction. </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="mitFilterList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the spacing to be applied to flights in FEA/FCA secondary filters. Present only if different spacing is to be applied to flows that are specified by FEA secondary filters.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="mitFilter" type="mitFilterType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Specifies one of three possible filter types: Destination airport, FXA secondary filter, or miles in trail. It is a single filter.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="mitFeaAsOneFilterList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the spacing to be applied to flights in FEA/FCA secondary filters. Present only if different spacing is to be applied to flows that are specified by FEA secondary filters.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="mitFeaAsOneFilterId" type="rriaIdType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for the spacing to be applied to flights in FEA/FCA secondary filters. Present only if different spacing is to be applied to flows that are specified by FEA secondary filters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="mitSegmentList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the reroute segments affected by this MIT. Will not be present if the MIT restriction is based on an FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="mitSegment" maxOccurs="unbounded">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="rerouteSegmentId">
                    <xsd:annotation>
                      <xsd:documentation>The ID of a reroute segment to which the MIT will be applied.  If a crossing segment was specified, TSD identifies each included reroute segment that the line crosses.  If a fix was specified, TSD identifies each included reroute segment that contains the fix.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[OD]\d{1,5}(\s[OD]\d{1,5})*"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="mitLocation" type="rriaLatLongType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>The location where the crossing segment crosses the reroute segment.  Comma-separated latitude and longitude in hundredths of degrees. 

This is the location where the MIT will be applied to flights on this segment.  This tag is present only if a crossing segment was specified for the MIT.  If a fix was specified, then the MIT will be applied at the fix, in the same location for all segments.
</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="mitFilterType">
    <xsd:annotation>
      <xsd:documentation>Defines the three possible filter types.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="mitDestinationFilterId" type="rriaIdType">
          <xsd:annotation>
            <xsd:documentation>Present only if the MIT filter is based on a destination airport.  For an MIT based on a crossing segment or fix, the ID will be the destination airport name.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="mitFeaFilterId" type="rriaIdType">
          <xsd:annotation>
            <xsd:documentation>Present only if the MIT filter is based on a FXA secondary filter.  System generated unique ID of a secondary filter associated with the FEA/FCA usually used for display purposes.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="mitFilterSpacingInMiles">
        <xsd:annotation>
          <xsd:documentation>The spacing to be applied to flights in this secondary filter in miles.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="EXCLUDE|\d{1,3}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="rriaIdType">
    <xsd:annotation>
      <xsd:documentation>Format used to define various IDs across RRIA.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z0-9\.]{0,64}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaLatLongType">
    <xsd:annotation>
      <xsd:documentation>Format used to send Latitude/Longitude pairs in RRIA messages. Both value in the pair should be in the "Hundred" format which means: 
			standard format(rounded to the hundredth decimal place) * 100</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="-?(9000|[0-8]?(\d){1,3}),[ ]?-?(18000|\d{1,4}|[0-1][0-7]\d{3})"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rerouteIdType">
    <xsd:annotation>
      <xsd:documentation>System generated unique identifier of reroute. Format is rr.{sitename}.{hostname}.{yyyyMMddhhmmss}</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="rr\.[A-Za-z0-9]{1,8}\.\w{1,8}\.\d{14}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="trajectorySourceType">
    <xsd:annotation>
      <xsd:documentation>Indicates the source of the trajectory data that TFMS used for the trajectory assignment.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TOS "/>
      <xsd:enumeration value="FP"/>
      <xsd:enumeration value="EIFP"/>
      <xsd:enumeration value="HIST"/>
      <xsd:enumeration value="MANUAL"/>
      <xsd:enumeration value="DIRECT"/>
      <xsd:enumeration value="REROUTE"/>
      <xsd:enumeration value="TACTICAL_REROUTE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ctopExemptCriteriaType">
    <xsd:annotation>
      <xsd:documentation>Data associated with CTOP exemption criteria</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="active">
        <xsd:annotation>
          <xsd:documentation>Defines if active flights should be exempt or not - TRUE = exempt</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:boolean"></xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="international">
        <xsd:annotation>
          <xsd:documentation>Defines if international flights should be exempt - TRUE = exempt</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:boolean"></xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="acidAny" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>One or more ACIDs separated by a space</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:list itemType="nxce:aircraftIdType"/>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="arrivesAny" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="arrivesAnyArpt" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Exempt arrival airport.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="([A-Z0-9]{3,5} )*([A-Z0-9]{3,5})"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="arrivesAnyArtcc" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Exempt arrival center.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="([A-Z0-9]{3,4} )*([A-Z0-9]{3,4})"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="departsAny" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="departsAnyArpt" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Exempt departure airport.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="([A-Z0-9]{3,5} )*([A-Z0-9]{3,5})"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="departsAnyArtcc" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Exempt departure center.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="([A-Z0-9]{3,4} )*([A-Z0-9]{3,4})"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="exemptByTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flight is exempt if it has ETD earlier than the time of CTOP issuance plus this value in minutes</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="999"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="fxaBinType">
    <xsd:annotation>
      <xsd:documentation>CTOP FCA time bin information</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="fcaBinTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Start time of FCA bin in a CTOP</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaBinCapacity" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>In ENTRY mode - the number of flights that can enter the FCA during the time bin. In PEAK mode - the number of flight they can be in the FCA at any one time during the time bin.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="225"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="fcaBinAutoRevision" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Whether the FCA time bin is enabled for automatic revision</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:boolean"></xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="fcaBinHighTrigger" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The demand that has to exceed capacity to trigger a CTOP allocation in automatic revision mode</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="99"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="fcaBinLowTrigger" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Amount of demand that has to fall below capacity to trigger a CTOP allocation in automatic revision mode</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="99"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:simpleType name="fcaRemarksType">
    <xsd:annotation>
      <xsd:documentation>Indicates the source of the trajectory data that TFMS used for the trajectory assignment.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NRP"/>
      <xsd:enumeration value="MEDEVAC"/>
      <xsd:enumeration value="CATIII"/>
      <xsd:enumeration value="ALTRV"/>
      <xsd:enumeration value="SWAP"/>
      <xsd:enumeration value="DVRSN"/>
      <xsd:enumeration value="ADCUS"/>
      <xsd:enumeration value="FCA"/>
      <xsd:enumeration value="WXRTE"/>
      <xsd:enumeration value="HAR"/>
      <xsd:enumeration value="PTP"/>
      <xsd:enumeration value="ICR"/>
      <xsd:enumeration value="RR"/>
      <xsd:enumeration value="HOSP"/>
      <xsd:enumeration value="AIREVAC"/>
      <xsd:enumeration value="DIVERT"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
