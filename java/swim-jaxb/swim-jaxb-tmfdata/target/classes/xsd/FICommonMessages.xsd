<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns="urn:us:gov:dot:faa:atm:tfm:ficommonmessages" 
            xmlns:fce="urn:us:gov:dot:faa:atm:tfm:ficommondatatypes" 
            xmlns:nxce="urn:us:gov:dot:faa:atm:tfm:tfmdatacoreelements" 
            targetNamespace="urn:us:gov:dot:faa:atm:tfm:ficommonmessages" 
            version="2.0.4" 
            elementFormDefault="qualified" 
            attributeFormDefault="unqualified">
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:ficommondatatypes" schemaLocation="FICommonDataTypes.xsd"/>
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:tfmdatacoreelements" schemaLocation="TFMDataCoreElements.xsd"/>
  <xsd:complexType name="baseFxaFilterType">
    <xsd:sequence>
      <xsd:choice minOccurs="0">
        <xsd:element name="traverseAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of center, sector and/or fix names, separated by spaces. Only flights that fly through any of these are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="(([A-Z0-9]{3,4})|([0-9A-Za-z]{2,3}|[A-Za-z][0-9A-Za-z]{3}|[0-9A-Za-z][A-Za-z][0-9A-Za-z]{2}|[0-9A-Za-z]{2}[A-Za-z][0-9A-Za-z]|[0-9A-Za-z]{3}[A-Za-z]|[0-9A-Za-z]{5})[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="traverseAll" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of center, sector and/or fix names, separated by spaces. Only flights that fly through all of these are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="(([A-Z0-9]{3,4})|([0-9A-Za-z]{2,3}|[A-Za-z][0-9A-Za-z]{3}|[0-9A-Za-z][A-Za-z][0-9A-Za-z]{2}|[0-9A-Za-z]{2}[A-Za-z][0-9A-Za-z]|[0-9A-Za-z]{3}[A-Za-z]|[0-9A-Za-z]{5})[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="traverseNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of center, sector and/or fix names, separated by spaces. Flights that fly through any of these are excluded from the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="(([A-Z0-9]{3,4})|([0-9A-Za-z]{2,3}|[A-Za-z][0-9A-Za-z]{3}|[0-9A-Za-z][A-Za-z][0-9A-Za-z]{2}|[0-9A-Za-z]{2}[A-Za-z][0-9A-Za-z]|[0-9A-Za-z]{3}[A-Za-z]|[0-9A-Za-z]{5})[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="useAirwayAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of airway names, separated by spaces. Only flights that fly on any of these are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="([A-Z0-9]{2,8}[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="useAirwayAll" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of airway names, separated by spaces. Only flights that fly on all of these are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="([A-Z0-9]{2,8}[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="useAirwayNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of airway names, separated by spaces. Flights that fly on any of these are excluded from the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="([A-Z0-9]{2,8}[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="locatedAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of center and/or sector names, separated by spaces. Only flights currently located within any of these centers or sectors are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="(([A-Z0-9]{3,4})|([0-9A-Za-z]{2,3}|[A-Za-z][0-9A-Za-z]{3}|[0-9A-Za-z][A-Za-z][0-9A-Za-z]{2}|[0-9A-Za-z]{2}[A-Za-z][0-9A-Za-z]|[0-9A-Za-z]{3}[A-Za-z]|[0-9A-Za-z]{5})[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="locatedNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of center and/or sector names, separated by spaces. Flights currently located within any of these centers or sectors are excluded from the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:pattern value="(([A-Z0-9]{3,4})|([0-9A-Za-z]{2,3}|[A-Za-z][0-9A-Za-z]{3}|[0-9A-Za-z][A-Za-z][0-9A-Za-z]{2}|[0-9A-Za-z]{2}[A-Za-z][0-9A-Za-z]|[0-9A-Za-z]{3}[A-Za-z]|[0-9A-Za-z]{5})[\s]?){1,100}"/>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="typeAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of aircraft types, separated by spaces. Only flights that match one of these are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="typeNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of aircraft types that are excluded from the FEA/FCA, separated by spaces.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:maxLength value="64"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="remarksAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of remarks, separated by spaces. Only flights that file any of these remarks are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="fcaRemarksType"/>
              </xsd:simpleType>
              <xsd:maxLength value="512"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="remarksAll" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of remarks, separated by spaces. Flights that file all of these remarks are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="fcaRemarksType"/>
              </xsd:simpleType>
              <xsd:maxLength value="512"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="remarksNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of remarks, separated by spaces. Flights that file any of these remarks are excluded in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="fcaRemarksType"/>
              </xsd:simpleType>
              <xsd:maxLength value="512"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="headingIs" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>A heading followed by a tolerance, separated by spaces. Only flights whose heading is within the specified tolerance of the specified heading are included in the FEA/FCA. The heading is degrees from 0 to 360. The tolerance is degrees from 0 to 180.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:pattern value="([0-9]|[1-9][0-9]|[1-2][0-9][0-9]|[3][0-6][0-9])\s([0-9]|[1-9][0-9]|[1][0-8][0-9])"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="headingNot" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>A heading followed by a tolerance, separated by spaces. Flights whose heading is within the specified tolerance of the specified heading are excluded from the FEA/FCA. The heading is degrees from 0 to 360. The tolerance is degrees from 0 to 180.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:pattern value="([0-9]|[1-9][0-9]|[1-2][0-9][0-9]|[3][0-6][0-9])\s([0-9]|[1-9][0-9]|[1][0-8][0-9])"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="acidAny" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of aircraft IDs (call signs), separated by spaces. Only flights with one of these IDs are included in the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:maxLength value="1024"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="acidNone" minOccurs="1">
          <xsd:annotation>
            <xsd:documentation>List of aircraft IDs (call signs), separated by spaces. Flights with one of these IDs are excluded from the FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction>
              <xsd:simpleType>
                <xsd:list itemType="xsd:string"/>
              </xsd:simpleType>
              <xsd:maxLength value="1024"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="secondaryAllFcaFilterType">
    <xsd:complexContent>
      <xsd:extension base="secondaryAnyFcaFilterType">
        <xsd:sequence>
          <xsd:element name="flightLevel" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>Upper flight level and lower flight level, separated by spaces. Only flights that are within the specified range at some time while they are in the FEA/FCA are included. Flight levels are in hundreds of feet(ddd).</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:pattern value="[0-9]?[0-9]?[0-9]\s[0-9]?[0-9]?[0-9]"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="aircraftCategoryAny" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>List of single-character aircraft type abbreviations
                        (J=Jet, P=Prop, T=Turbo), separated by spaces. Only flights that match one of these are included in the FEA/FCA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction>
                <xsd:simpleType>
                  <xsd:list itemType="xsd:string"/>
                </xsd:simpleType>
                <xsd:pattern value="[JPT ]{1,6}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="weightClassAny" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>List of single-character weight class abbreviations
                        (H=Heavy, L=Large, S=Small), separated by spaces. Only flights that match one of these are included in the FEA/FCA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction>
                <xsd:simpleType>
                  <xsd:list itemType="xsd:string"/>
                </xsd:simpleType>
                <xsd:pattern value="[HLS ]{1,6}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="userCategoryAny" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>List of single-character user category abbreviations
                        (T=Air Taxi, F=Cargo, C=Commercial, G=General Aviation, M=Military), separated by spaces. Only flights that match one of these are included in the FEA/FCA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction>
                <xsd:simpleType>
                  <xsd:list itemType="xsd:string"/>
                </xsd:simpleType>
                <xsd:pattern value="[TFCGM ]{1,10}"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="status" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>One of ACTIVE, PROPOSED. If ACTIVE, FEA/FCA includes only airborne flights. If PROPOSED, FEA/FCA includes only flights currently on the ground.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="ACTIVE"/>
                <xsd:enumeration value="PROPOSED"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="RVSM" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>One of COMPLIANT, NON-COMPLIANT. If COMPLIANT, the FEA/FCA includes only flights that are RVSM compliant at the time they are in the FEA/FCA. If NONCOMPLIANT, the FEA/FCA includes only flights that are RVSM non-compliant at the time they are in the FEA/FCA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="COMPLIANT"/>
                <xsd:enumeration value="NON-COMPLIANT"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:choice minOccurs="0">
            <xsd:element name="departureTimeRange" minOccurs="1" type="fce:dateTimeRangeType">
              <xsd:annotation>
                <xsd:documentation>Begin-time and end-time4, separated by spaces. Only flights whose departure times fall within this time range are included in the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="arrivalTimeRange" minOccurs="1" type="fce:dateTimeRangeType">
              <xsd:annotation>
                <xsd:documentation>Begin-time and end-time4, separated by spaces. Only flights whose arrival times fall within this time range are included in the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="secondaryAnyFcaFilterType">
    <xsd:complexContent>
      <xsd:extension base="baseFxaFilterType">
        <xsd:sequence>
          <xsd:choice minOccurs="0">
            <xsd:element name="departsAny" minOccurs="1">
              <xsd:annotation>
                <xsd:documentation>List of center and/or airport names, separated by spaces. Only flights departing one of these are included in the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="((([a-zA-Z]{3,7})|([A-Z0-9]{3,4})) *){1,100}"/>
                  <xsd:maxLength value="64"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="departsNone" minOccurs="1">
              <xsd:annotation>
                <xsd:documentation>List of center and/or airport names, separated by spaces. Flights departing any of these are excluded from the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="((([a-zA-Z]{3,7})|([A-Z0-9]{3,4})) *){1,100}"/>
                  <xsd:maxLength value="64"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:choice>
          <xsd:choice minOccurs="0">
            <xsd:element name="arrivesAny" minOccurs="1">
              <xsd:annotation>
                <xsd:documentation>List of center and/or airport names, separated by spaces. Only flights arriving one of these are included in the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="((([a-zA-Z]{3,7})|([A-Z0-9]{3,4})) *){1,100}"/>
                  <xsd:maxLength value="64"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="arrivesNone" minOccurs="1">
              <xsd:annotation>
                <xsd:documentation>List of center and/or airport names, separated by spaces. Flights arriving any of these are excluded from the FEA/FCA.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="((([a-zA-Z]{3,7})|([A-Z0-9]{3,4})) *){1,100}"/>
                  <xsd:maxLength value="64"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:choice>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="primaryAllFcaFilterType">
